package empleados;

import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		
		ArrayList<Empleado> empleados = new ArrayList<>();
		
		for(int i=0; i<10; i++){
			empleados.add(new Empleado(Integer.toString(i)));
		}
		
		for(Empleado emp:empleados)
			System.out.println("Empleado" + emp.presentarFijo());
			
	}

}
