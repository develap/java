package clases.internas;

public class ClaseInterna {
	
	ClaseInterna(){
		new ClasePrivada();
		new ClaseProtegida();
		new ClasePublica();
	}

	private class ClasePrivada{
		ClasePrivada(){ System.out.println("Clase Privada");}
	}

	protected class ClaseProtegida{
		ClaseProtegida(){ System.out.println("Clase Protegida");}
	}
	
	public class ClasePublica{
		ClasePublica(){System.out.println("Clase Publica");};
	}
	
	
	public static void main(String[] args) {
		new ClaseInterna();
	}
	
}
