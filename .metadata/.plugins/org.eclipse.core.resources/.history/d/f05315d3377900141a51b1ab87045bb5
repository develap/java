package Fruticola;

public class Melocoton extends Fruta{
	
	/**
	 * @param precio
	 * @param color
	 * @param azucar
	 */
	public Melocoton(double precio, String color, double azucar) {
		super();
		this.precio = precio;
		this.color = color;
		this.azucar = azucar;
	}

	
	public static String vender(Fruta fruta){
		contador++;
		System.out.println("Melocoton");
		return "Vender melocoton";
	}
}
