package empleados;

public class EmpleadosAsalariados extends Empleado{

	private Integer sueldoFijo;

	/**
	 * @param sueldoFijo
	 */
	public EmpleadosAsalariados(Integer sueldoFijo) {
		super();
		this.sueldoFijo = sueldoFijo;
	}

	public Integer getSueldoFijo() {
		return sueldoFijo;
	}

	public void setSueldoFijo(Integer sueldoFijo) {
		this.sueldoFijo = sueldoFijo;
	}
}
