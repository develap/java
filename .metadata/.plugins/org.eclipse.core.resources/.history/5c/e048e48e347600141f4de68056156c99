package Fruticola;

public class Fuji extends Manzana{

	private double precio;
	private String color;
	private double azucar;
	
	
	/**
	 * @param precio
	 * @param color
	 * @param azucar
	 */
	public Fuji(double precio, String color, double azucar) {
		super();
		this.precio = precio;
		this.color = color;
		this.azucar = azucar;
	}
	

	public double getPrecio() {
		return precio;
	}
	
	public void setPrecio(double precio) {
		this.precio = precio;
	}
	
	public String getColor() {
		return color;
	}
	
	public void setColor(String color) {
		this.color = color;
	}
	
	public double getAzucar() {
		return azucar;
	}
	
	public static void setAzucar(double azucar) {
		contador++;
		System.out.println("Fuji");
		this.azucar = azucar;
	}
		
}
