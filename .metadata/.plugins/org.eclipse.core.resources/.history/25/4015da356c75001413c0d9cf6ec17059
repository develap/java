package principios;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Vehiculo coche = new Vehiculo("ford", "fiesta", "rojo", "8787");
		Vehiculo coche1 = new Vehiculo("bmw", "", "blanco", "999");
		coche.setColor("verde");
		
		Object obj = coche;
		coche1 = coche;		
		System.out.println(coche);
		System.out.println(obj);
		System.out.println(coche1);
		
		Motocicleta moto = new Motocicleta("debi", "antorcha", "rojo", "AL343");		
				
		System.out.println(obj instanceof Vehiculo);
		System.out.println(coche instanceof Vehiculo);
		System.out.println(coche1 instanceof Vehiculo);
		System.out.println(moto instanceof Vehiculo);
	}

}
